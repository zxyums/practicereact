const _ = require('lodash');

const data = [
  { id: 1, name: 'Item 1', updatedDate: '2025-01-10' },
  { id: 2, name: 'Item 2', updatedDate: '' },
  { id: 3, name: 'Item 3', updatedDate: '2025-01-15' },
  { id: 4, name: 'Item 4', updatedDate: null },
];

const sortedData = _.sortBy(data, (item) => {
  return item.updatedDate ? new Date(item.updatedDate) : Infinity;
});

console.log(sortedData);